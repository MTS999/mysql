<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Car List</title>
</head>
<body>
  <h1>Car List</h1>
  <table border="1">
    <thead>
      <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Model</th>
        <th>Year</th>
        <th>Price</th>
        <th>Rent Per Day</th>
      </tr>
    </thead>
    <tbody>
      <% carsData.forEach(car => { %>
        <tr>
          <td><%= car.id %></td>
          <td><%= car.name %></td>
          <td><%= car.model %></td>
          <td><%= car.year %></td>
          <td><%= car.price %></td>
          <td><%= car.rentPerDay %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
  <a href="/">Home</a>
</body>
</html>
